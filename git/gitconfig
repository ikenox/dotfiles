[include]
    path = ~/.gitconfig.local
[user]
	useConfigOnly = true
[alias]
	co = checkout
	st = status
	ci = commit -a
	di = diff
	br = branch
    graph = log --graph --date=short --decorate=full --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
[core]
    excludesfile = ~/.gitignore
	editor = vim -c \"set fenc=utf-8\"
[push]
	default = simple
[ghq]
    vcs = git
[diff]
	algorithm = patience
